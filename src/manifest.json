{
    "manifest_version": 2,
    "name": "franco_extension",
    "version": "1.0.0",
    "description": "TBD",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*","file:///*/*"],
        "run_at": "document_idle",
        "js": ["content.js"]
      }
    ]
}